    "esModuleInterop": true,
    "noImplicitAny": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "../dist",
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    },
    "types": [ "@pyoner/svelte-types" ]
  },
  "include": [ "server.ts" ]
}