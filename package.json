  "description": "",
  "main": "index.js",
  "module": "public/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently --kill-others-on-fail -n build,run \"npm:dev\" \"npm:server:delay_run\"",
    "dev": "rollup -c -w --both",
    "build": "npm run build1 && npm run server:build",
    "build2": "webpack",
    "build1": "rollup -c",
    "server:delay_run": "npm run server:public",
    "server": "npm run server:dev",
    "server:cw": "rollup -c -w --server",
    "server:public": "nodemon -w public/server.js public/server.js",
    "server:dev": "concurrently -k -n build,run \"npm:server:cw\" \"nodemon --delay 1 public/server.js\"",
    "server:dev2": "tsnd --project server/tsconfig.json server/server.ts",
    "server:dev1": "nodemon --watch server -e ts,js --exec \"npm run server:run1\"",
    "server:run": "ts-node --project server/tsconfig.json server/server.ts",
    "server:run1": "npm run server:build && node dist/server.js",
    "server:build": "npm run server:build1",
    "server:build2": "tsc -p server",
    "server:build1": "rollup -c --server",
    "serve:dist": "python -m http.server --directory dist"
  },
